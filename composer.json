{
    "name": "aerocommerce/setup",
    "description": "Aero Commerce Store Onboarding Process.",
    "keywords": [
        "aero",
        "ecommerce"
    ],
    "autoload": {
        "psr-4": {
            "Aero\\Setup\\": "src/"
        }
    },
    "require": {
        "php": "^7.2|^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0",
        "guzzlehttp/guzzle": "^6.3.1|^7.0.1",
        "symfony/yaml": "^5.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Aero\\Setup\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
